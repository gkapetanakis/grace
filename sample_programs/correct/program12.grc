$$
  Description:
      Check reading and writing of characters, integers and strings
  Purpose:
      check that our runtime IO functions work correctly
$$

fun doSomething() : nothing
  var c: char;
  var i: int;
  var s: char[10];
{
  c <- readChar();
  i <- readInteger();
  readString(10, s);

  writeChar(c);
  writeChar('\n');
  writeInteger(i);
  writeChar('\n');
  writeString(s);
  writeChar('\n');
}
